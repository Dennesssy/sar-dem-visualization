{
  "name": "sar-dem-visualization",
  "version": "1.0.0",
  "description": "React + Node.js application for SAR and DEM data visualization using Three.js for structure identification and analysis",
  "main": "server/server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently \"nodemon server/server.js\" \"cd client && npm start\"",
    "install-deps": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "SAR",
    "DEM",
    "visualization",
    "three.js",
    "react",
    "node.js",
    "geospatial",
    "remote-sensing",
    "data-visualization",
    "structure-identification"
  ],
  "author": "Dennesssy",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "axios": "^1.6.0"
  }
}
